		<dc:title id="title">The Federalist Papers</dc:title>
		<meta property="file-as" refines="#title">Federalist Papers, The</meta>
		<dc:subject id="subject-1">Constitutional history -- United States -- Sources</dc:subject>
		<dc:subject id="subject-2">Constitutional law -- United States</dc:subject>
		<meta property="authority" refines="#subject-1">LCSH</meta>
		<meta property="term" refines="#subject-1">sh2008101624</meta>
		<meta property="authority" refines="#subject-2">LCSH</meta>
		<meta property="term" refines="#subject-2">sh85139986</meta>
